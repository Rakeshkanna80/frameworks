<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.rak.rakapp.init.Water" id="water">
		<constructor-arg value="2.5f" />
	</bean>
	<bean class="com.rak.rakapp.init.Barber" id="barber">
		<constructor-arg value="Nikunj"></constructor-arg>
		<constructor-arg value="5" />
	</bean>
	<bean class="com.rak.rakapp.init.Trimmer" id="trimmer">
		<constructor-arg type="String" value="MI"></constructor-arg>
		<constructor-arg  type="String" value="1100"></constructor-arg>
	</bean>
	<bean class="com.rak.rakapp.init.Trimmer" id="trimmer1">
		<constructor-arg type="int" value="110"></constructor-arg>
		<constructor-arg  type="String" value="philiphs"></constructor-arg>
	</bean>



</beans>